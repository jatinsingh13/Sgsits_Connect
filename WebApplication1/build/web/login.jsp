        <title>Login</title>
    </head>
    <body>
        <!--navbar-->
        <%@include file="normalnavbar.jsp"%>
       
        <div class="login">
            <div class="login-box">
                <h1 style="text-align: center; font-size: 46px;">Login</h1>
                <form style="margin-top: 15px;">
                    <p>Username</p>
                    <input class="special" type="text" name="" placeholder="Enter Username">
                    <p>Password</p>
                    <input class="special" type="password" name="" placeholder="Enter Password">
                    <input class="submit" type="submit" name="" value="Login">
                </form>
            </div>
        </div>
        
        <script src="https://code.jquery.com/jquery-3.6.0.slim.min.js" integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI=" crossorigin="anonymous"></script>
